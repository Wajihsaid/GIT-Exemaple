pipeline{
    agent any
    tools{
        jdk 'JDK21'
    }
    stages{
        stage('Checkout code')
        {
            steps{
                git branch: 'master', url:'https://github.com/Wajihsaid/GIT-Exemaple.git'
            }
        }
        stage('Compile code')
        {
            steps{
                sh 'javac -d out src/main/java/org/example/*.java'
            }
        }
        stage('Excute code')
        {
            steps{
                sh 'java -cp out org.example.Main'
            }
        }
         stage('disply message')
        {
            steps{
                echo "hello from github"
            }
        }
    }
}
